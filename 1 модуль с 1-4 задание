#2
city=input()
time=input()
print(f'Current location is {city} and time is {time} ')

#3
a=int(input())
b=int(input())
c=input()
d=True
while c !="":
    if int(c) not in range(a,b+1):
        d = False
    c=input()
print(d)

#4
a=input()
b=False
if ("a" in a or "o" in a) and ("i" not in a and "e" not in a):
    b = True
print(b)
